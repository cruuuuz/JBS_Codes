    "import mouse\n",
    "from time import sleep\n",
    "\n",
    "'''\n",
    "Simula o click do mouse para manter o servidor ativo\n",
    "'''\n",
    "\n",
    "c = 0\n",
    "\n",
    "while True:\n",
    "    mouse.move(100, 100, absolute=True, duration=0.1)\n",
    "    mouse.click('left')\n",
    "    sleep(3600)\n",
    "    c += 1\n",
    "    print(f'Essa foi a {c} movimentação')"
   ]
  }
